
public class SubGrids {

	public static void main(String[] args) {
		int [][] arr = {{-1,1},{10,2}};
		int m = 2, n = 2;
		
		for (int r = 0; r < m; r++) {
			for (int c = 0; c < n; c++) {
				int sum = 0;
				for (int i = 0; i < m-r; i++) {
					for (int j = 0; j < c-r; j++) {
						sum += arr[i][j];
					}
				}
				System.out.println(sum);
			}
		}
	}

}
