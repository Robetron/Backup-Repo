
public class SubGrids {

	public static void main(String[] args) {
		int [][] arr = {{1,2,3},{4,5,6}};
		int m = 2, n = 3;
		int sum = 0;
		for (int r = 1; r <= m; r++) {
			for (int c = 1; c <= n; c++) {
				int temp = subGridTotal(r, c, m, n, arr);
				if(sum < temp) sum = temp;
			}
		}
	}

	private static int subGridTotal(int r, int c, int m, int n, int arr[][]) {
		for (int i = 0; i < r; i++) {
			for (int j = 0; j < c; j++) {
				
			}
		}
		return 0;
	}

}
